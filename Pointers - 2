#include <stdio.h>

int main() {
    
    int num = 10;
    int *ptr = &num;
    
    printf ("Address of num = %u\n ",&num);
    printf ("Value of num = %u\n ",ptr);
    printf ("Address of pointer = %u\n ",&ptr);
    printf ("Value of ptr = %d\n ",*ptr);
    printf ("Value of num = %d\n ",num);
    return 0;
}


/* Address of num = 638800780
 Value of num = 638800780
 Address of pointer = 638800768
 Value of ptr = 10
 Value of num = 10
 

=== Code Execution Successful ===*/
