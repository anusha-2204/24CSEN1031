#include <stdio.h>
int main() {
    int a, b, c, sum = 0;
    printf("Enter any number to check whether it is an Armstrong number or not:");
    scanf("%d",&c);
    a = c;
    while(a>=1) {
        b= a%10;
        a = a/10;
        sum =sum+b*b*b;
        
  }
     if (sum == c) {
         printf("\n\nEntered number is an Armstrong  Number");
     }
    else{
        printf("\n\nthe entered number is NOT an armstrong number");
    }     
     
      return 0;
}


/*OUTPUT

Enter any number to check whether it is an Armstrong number or not:531


the entered number is NOT an armstrong number

=== Code Execution Successful ===*/
